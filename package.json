{
  "name": "doc-referral-mgmt",
  "version": "0.1.0",
  "private": true,
  "author": "Rajnikant Gupta (capricorn.raj@gmail.com)",
  "dependencies": {
    "@material-ui/core": "^4.5.2",
    "@material-ui/icons": "^4.5.1",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "chartist": "0.10.1",
    "classnames": "2.2.6",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "electron-is-dev": "^1.1.0",
    "express": "^4.17.1",
    "history": "4.9.0",
    "http-errors": "^1.7.3",
    "moment": "^2.24.0",
    "mongoose": "^5.7.7",
    "mongoose-sequence": "^5.2.2",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.4",
    "perfect-scrollbar": "1.4.0",
    "prop-types": "15.7.2",
    "ra-data-json-server": "^2.9.8",
    "ra-data-simple-rest": "^2.9.8",
    "react": "^16.11.0",
    "react-admin": "^2.9.8",
    "react-chartist": "0.13.3",
    "react-dom": "^16.11.0",
    "react-router-dom": "5.0.1",
    "react-scripts": "3.2.0",
    "slugify": "^1.3.5",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "electron": "^7.0.0",
    "electron-builder": "^22.1.0",
    "eslint": "^6.6.0",
    "jest": "^24.9.0",
    "wait-on": "^3.3.0"
  },
  "build": {
    "appId": "org.rk.apps.drms",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": "NSIS"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "publish": {
      "provider": "https://github.com/rkgupta/doc-referral-mgmt.git"
    }
  },
  "main": "public/electron.js",
  "homepage": "./",
  "proxy": "http://localhost:8080/",
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "postinstall": "install-app-deps",
    "release": "yarn react-build && electron-builder --publish=always",
    "build": "yarn react-build && yarn electron-build",
    "start": "concurrently \"nodemon server/server.js\" \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
